{
	"name": "interaction-kit-workspace",
	"workspaces": [
		"packages/*"
	],
	"version": "0.0.1",
	"private": true,
	"packageManager": "npm@8.5.1",
	"scripts": {
		"cli:create": "npm run dev --workspace=create-ikit-app",
		"build": "NODE_OPTIONS=--openssl-legacy-provider turbo run build",
		"typecheck": "turbo run typecheck",
		"prettier": "turbo run prettier",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"version": "npx lerna version --no-push && git commit -am \"Version Bump\" npm i"
	},
	"engines": {
		"npm": ">=8.0.0",
		"node": ">=16.6.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.0.0",
		"@types/cross-spawn": "^6.0.2",
		"@types/debug": "^4.1.7",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.9.6",
		"@types/service-worker-mock": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-xo": "^0.38.0",
		"eslint-config-xo-typescript": "^0.44.0",
		"jest": "^27.2.2",
		"lerna": "^4.0.0",
		"prettier": "^2.4.1",
		"service-worker-mock": "^2.0.5",
		"ts-jest": "^27.0.5",
		"tsdx": "^0.14.1",
		"tslib": "^2.3.1",
		"tsup": "^5.11.10",
		"turbo": "^1.0.24",
		"typescript": "^4.6.2"
	}
}
