{
	"name": "interaction-kit",
	"version": "0.1.2",
	"author": "Ian Mitchell",
	"description": "Interaction Kit is a Discord HTTP API Framework",
	"license": "MIT",
	"exports": "./dist/src/index.js",
	"typings": "dist/src/index.d.ts",
	"type": "module",
	"scripts": {
		"start": "node .",
		"build": "tsc",
		"clean": "rm -r dist/",
		"typecheck": "tsc --noEmit",
		"prettier": "prettier src/**/*.ts --check && prettier bin/*.ts",
		"lint": "eslint ./src/**/*.ts && eslint ./bin/*.ts",
		"test": "jest"
	},
	"files": [
		"dist",
		"src"
	],
	"bin": {
		"ikit": "./dist/bin/ikit.js"
	},
	"engines": {
		"node": ">=16.6.0"
	},
	"dependencies": {
		"arg": "^5.0.1",
		"chokidar": "^3.5.2",
		"discord-request": "file:../discord-request",
		"dotenv": "^10.0.0",
		"fastify": "^3.17.0",
		"fastify-raw-body": "^3.1.0",
		"ngrok": "^4.2.2",
		"node-fetch": "^2.6.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IanMitchell/interaction-kit.git"
	},
	"keywords": [],
	"bugs": {
		"url": "https://github.com/IanMitchell/interaction-kit/issues"
	},
	"homepage": "https://github.com/IanMitchell/interaction-kit#readme"
