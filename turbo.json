{
	"baseBranch": "origin/main",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"bot": {
			"dependsOn": ["build"],
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"typecheck": {
			"dependsOn": ["build"]
		},
		"prettier": {},
		"lint": {
			"dependsOn": ["build"]
		},
		"test": {
			"dependsOn": ["^build"]
		}
	}
}
